<odoo>

<template
        id="report_settlement_document_percentage_col"
        inherit_id="commission.report_settlement_document"
        priority="15"
    >
    <xpath expr="//th[3]" position="before">
        <th class="text-start">Percentage</th>
    </xpath>
    <xpath expr="//td[3]" position="before">
        <td class="text-start">
            <span t-esc="l.percentage" /> %
        </td>
    </xpath>
</template>

</odoo>
